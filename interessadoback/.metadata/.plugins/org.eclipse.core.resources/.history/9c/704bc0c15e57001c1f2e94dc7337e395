CREATE TABLE discipulador (
    codigo SERIAL,
    nome VARCHAR(50) NOT NULL,
    codigo_ocupacao int not NULL
    codigo_discipulado int not NULL,
    FOREIGN KEY (codigo_ocupacao) REFERENCES ocupadao(codigo),
    FOREIGN KEY (codigo_discipulado) REFERENCES discipulado(codigo),
    PRIMARY KEY(codigo)
);

